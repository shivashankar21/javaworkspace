public class MultipleInheritanceDemo {
	public static void main(String[] args) {
		College c1=new College("satyam", 500, "Coforge", "MCA");

		Hostel h1=new Hostel("Ram", "Block A");

		StudentDetails s1 = new StudentDetails();
		
		s1.collegeDetails();
		s1.studentData();
		
		s1.hostelDetail();
		s1.studentRecord();
	}

}
